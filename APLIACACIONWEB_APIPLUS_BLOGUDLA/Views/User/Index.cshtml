@model IEnumerable<APLIACACIONWEB_APIPLUS_BLOGUDLA.Models.User>

@{
    ViewData["Title"] = "Users";
}

<h1>Users</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>

<table class="table">
    <thead>
        <tr>
            <th>@Html.DisplayNameFor(model => model.Username)</th>
            <th>@Html.DisplayNameFor(model => model.Nombres)</th>
            <th>@Html.DisplayNameFor(model => model.Apellidos)</th>
            <th>@Html.DisplayNameFor(model => model.CorreoElectronico)</th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@Html.DisplayFor(modelItem => item.Username)</td>
                <td>@Html.DisplayFor(modelItem => item.Nombres)</td>
                <td>@Html.DisplayFor(modelItem => item.Apellidos)</td>
                <td>@Html.DisplayFor(modelItem => item.CorreoElectronico)</td>
                <td>
                    <a asp-action="Edit" asp-route-username="@item.Username">Edit</a> |
                    <a asp-action="Details" asp-route-username="@item.Username">Details</a> |
                    <a asp-action="Delete" asp-route-username="@item.Username">Delete</a>
                </td>
            </tr>
        }
    </tbody>
</table>
